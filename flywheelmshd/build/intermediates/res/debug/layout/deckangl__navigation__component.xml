<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gcg="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="@color/gcg__dialog__background"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/navigtion_button__top__left"
            android:layout_width="64dp"
            android:layout_height="45dp"
            android:layout_marginLeft="50dp"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___portobello"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/navigtion_button__top__middle"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1.0"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___portobello"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/navigtion_button__top__right"
            android:layout_width="64dp"
            android:layout_height="45dp"
            android:layout_marginRight="50dp"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___portobello"
            tools:ignore="ButtonStyle" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:baselineAligned="false">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/navigtion_button__left__top"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___portobello" />

            <Button
                android:id="@+id/navigtion_button__left__middle"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginBottom="15dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___pink" />

            <Button
                android:id="@+id/navigtion_button__left__bottom"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___pink" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:orientation="vertical" >

        <ImageView
            android:id="@+id/annotated_deckangl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:contentDescription="@string/deckangl_tm__annotated_glyph"
            android:scaleType="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/navigtion_button__right__top"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___blue" />

            <Button
                android:id="@+id/navigtion_button__right__middle"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___pink" />

            <Button
                android:id="@+id/navigtion_button__right__bottom"
                android:layout_width="45dp"
                android:layout_height="64dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/gcg__button_state_list__deckangl_navigation__vertical___pink" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/navigtion_button__bottom__left"
            android:layout_width="64dp"
            android:layout_height="45dp"
            android:layout_marginLeft="50dp"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___blue"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/navigtion_button__bottom__middle"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1.0"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___yellow"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/navigtion_button__bottom__right"
            android:layout_width="64dp"
            android:layout_height="45dp"
            android:layout_marginRight="50dp"
            android:background="@drawable/gcg__button_state_list__deckangl_navigation__horizontal___red"
            tools:ignore="ButtonStyle" />
    </LinearLayout>

</LinearLayout><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/deckangl__navigation__component.xml --><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmshd/build/intermediates/exploded-aar/flywheelms-workbench/flywheelmslibraryhd/unspecified/res/layout/deckangl__navigation__component.xml -->