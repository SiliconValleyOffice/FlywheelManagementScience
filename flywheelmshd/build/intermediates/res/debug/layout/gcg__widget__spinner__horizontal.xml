<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/GcgWidgetRowHorizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_vertical" >
    
    <TextView
        android:id="@+id/widget_label"
        style="@style/GcgWidgetLabelHorizontalContainer"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true" />
    
    <TextView
        android:id="@+id/widget_fdk_cursor"
        android:layout_width="6dp"
        android:layout_height="35dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"
        android:background="@color/Orange" />
    
    <com.flywheelms.library.gcg.widget.GcgSpinner
        android:id="@+id/widget_spinner"
        style="@style/GcgWidgetData"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginRight="0dp"
        android:layout_toRightOf="@id/widget_label"
        android:layout_toLeftOf="@id/widget_fdk_cursor"
        android:layout_centerVertical="true" />

</RelativeLayout>
