<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/portobello"
    android:gravity="center_horizontal"
    android:orientation="vertical" >

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/sands_of_time"
        android:orientation="vertical" >

        <TableRow
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="15dp" >

            <TextView
                android:id="@+id/search_type__label"
                style="@style/NodeLabel"
                android:layout_gravity="left"
                android:layout_span="3"
                android:paddingLeft="9dp"
                android:text="Must contain all..."
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:paddingBottom="9dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp" >

            <EditText
                android:id="@+id/search_criteria"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_span="3"
                android:background="@color/WhiteSmoke"
                android:hint="enter search criteria"
                android:padding="6dp" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:gravity="center_vertical"
            android:paddingBottom="3dp"
            android:paddingLeft="15dp"
            android:paddingTop="20dp" >

            <ImageView
                android:id="@+id/node_list__icon"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/gcg__unspecified_glyph" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/node_type__label"
                    style="@style/NodeLabel"
                    android:paddingLeft="9dp"
                    android:text="Unspecified Node Type"
                    android:textColor="@color/black" />

                <TextView
                    android:id="@+id/list_action__label"
                    style="@style/NodeLabel"
                    android:text=" (select one)"
                    android:textColor="@color/yellow" />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:paddingBottom="10dp"
            android:paddingLeft="15dp" >

            <ListView
                android:id="@+id/node_list"
                style="@style/NodeData"
                android:layout_width="160dp"
                android:layout_height="345dp"
                android:layout_span="3"
                android:background="@color/WhiteSmoke"
                android:fadeScrollbars="false"
                android:hint="search results" />
        </TableRow>
    </TableLayout>

</LinearLayout><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/fms__node_picker.xml --><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmshd/build/intermediates/exploded-aar/flywheelms-workbench/flywheelmslibraryhd/unspecified/res/layout/fms__node_picker.xml -->