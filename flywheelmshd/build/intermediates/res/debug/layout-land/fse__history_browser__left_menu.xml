<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/gcg__left_menu__width"
    android:layout_height="match_parent"
    android:id="@+id/history_browser__left_menu"
    android:background="@color/gcg__thumb_menu__background"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="left"
        android:orientation="vertical" >

        <Button
            android:id="@+id/fse__history_browser__story__button"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/menu_button__bottom_margin"
            android:layout_marginTop="22dp"
            android:drawableStart="@drawable/button_state_list__fse__story"
            android:onClick="onClickDocumentStory" />

        <Button
            android:id="@+id/fse_button__notes"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/menu_button__bottom_margin"
            android:layout_marginTop="11dp"
            android:drawableStart="@drawable/button_state_list__fse__notes"
            android:onClick="onClickDocumentNotes" />
    </LinearLayout>

    <TextView
        android:id="@+id/left_menu__filler"
        android:layout_width="6dp"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        android:background="@color/gcg__tree_view__background" />

</LinearLayout><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout-land/fse__history_browser__left_menu.xml --><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmshd/build/intermediates/exploded-aar/flywheelms-workbench/flywheelmslibraryhd/unspecified/res/layout-land/fse__history_browser__left_menu.xml -->