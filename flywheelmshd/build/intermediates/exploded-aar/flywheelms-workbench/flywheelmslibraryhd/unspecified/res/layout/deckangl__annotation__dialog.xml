<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gcg="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical" >

    <com.flywheelms.library.fms.widget.text_view.FmmNodeTypeWidgetTextView
        android:id="@+id/fmm_node__type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        gcg:labelWidth="180" />

    <com.flywheelms.library.fms.widget.text_view.HeadlineWidgetTextView
        android:id="@+id/headline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        gcg:labelWidth="180"
        gcg:onClick="disabled"
        gcg:onLongClick="disabled" />

    <ImageView
        android:id="@+id/annotated_deckangl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:background="@drawable/gcg__layout_border__portobello"
        android:contentDescription="@string/deckangl_tm__annotated_glyph"
        android:padding="7dp"
        android:scaleType="center" />

</LinearLayout><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/deckangl__annotation__dialog.xml -->