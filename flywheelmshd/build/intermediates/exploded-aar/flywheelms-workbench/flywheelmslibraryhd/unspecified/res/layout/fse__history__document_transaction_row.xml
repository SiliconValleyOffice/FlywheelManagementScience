<?xml version="1.0" encoding="utf-8"?>
<com.flywheelms.library.fse.views.FseDocumentTransactionRowView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/fse__transaction_number__data"
            android:layout_width="@dimen/fse__transaction_number__width"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center_horizontal"
            android:paddingTop="13dp"
            android:text="timestamp"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_timestamp__data"
            android:layout_width="@dimen/fse__transaction_timestamp__width"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="12-31-1970"
            android:textSize="11.0dp" />

        <LinearLayout
            android:layout_width="@dimen/fse__transaction_community_member__width"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

	        <TextView
	            android:id="@+id/fse__transaction_community_member__data"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:gravity="center_horizontal"
	            android:paddingTop="7dp"
	            android:text="Steve Stamps"
	            android:textSize="12.0dp" />

	        <TextView
	            android:id="@+id/fse__transaction_comment__data"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:gravity="center_horizontal"
	            android:text="Some random comment."
	            android:textSize="11.0dp"
	            android:textStyle="bold" />
        </LinearLayout>

        <TextView
            android:id="@+id/fse__transaction_type__data"
            android:layout_width="@dimen/fse__transaction_type__width"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:paddingTop="13dp"
            android:text="Create"
            android:textSize="12.0dp" />

        <LinearLayout
            android:id="@+id/fse__document_body_audit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/fse__document_body_audit__story"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/fse__document_section__story"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingTop="7dp"
                    android:text="Story"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_new__data__story"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingTop="7dp"
                    android:text="0"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_modified__data__story"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingTop="7dp"
                    android:text="0"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_deleted__data__story"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingTop="7dp"
                    android:text="0"
                    android:textSize="12.0dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/fse__document_body_audit__notes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/fse__document_section__notes"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="Notes"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_new__data__notes"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="0"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_modified__data__notes"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="0"
                    android:textSize="12.0dp" />

                <TextView
                    android:id="@+id/fse__transaction_paragraph_deleted__data__notes"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="15dp"
                    android:paddingBottom="2dp"
                    android:text="0"
                    android:textSize="12.0dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    
	<include
	    layout="@layout/fms__list_item_divider" />

</com.flywheelms.library.fse.views.FseDocumentTransactionRowView><!-- From: file:/home/sstamps/AndroidDevelopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/fse__history__document_transaction_row.xml -->