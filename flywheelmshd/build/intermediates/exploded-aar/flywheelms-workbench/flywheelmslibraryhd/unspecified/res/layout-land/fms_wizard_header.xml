<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/gcg__tree_view__background"
    android:orientation="horizontal" >
    
    	<ImageView
    	    android:layout_width="wrap_content"
    	    android:layout_height="wrap_content"
    	    android:layout_marginRight="5dp"
    	    android:layout_marginBottom="2dp"
    	    android:gravity="center"
    	    android:src="@drawable/wizard_target" />
	    
	    <ImageView
	        android:id="@+id/fmm_node__image"
	        android:layout_width="70dp"
	        android:layout_height="70dp"
	        android:layout_gravity="center_vertical"
	        android:paddingLeft="15dp"
	        android:src="@drawable/fmm_noun__fiscal_year__gray"
	        android:contentDescription="FMM Node glyph" />
	    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
    		android:orientation="vertical" >
	        
	        <TextView 
	        	android:id="@+id/fmm_noun__name"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:paddingLeft="20dp"
		        android:textSize="22dp"
		        android:textStyle="bold|italic"
		        android:text="Fiscal Year" />
	        
	        <TextView 
	        	android:id="@+id/fmm_noun__headline"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:paddingLeft="18dp"
		        android:textColor="@color/gcg__widget_label__text_color"
		        android:textSize="24dp"
		        android:text="2013" />
	        
	    </LinearLayout>
	    
</LinearLayout>
