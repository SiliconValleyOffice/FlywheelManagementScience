<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_blue"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/fse__transaction__headings"
        android:layout_width="match_parent"
        android:layout_height="27dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/fse__transaction_number__heading"
            android:layout_width="@dimen/fse__transaction_number__width"
            android:layout_height="match_parent"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_number__heading"
            android:textColor="@color/gcg__thumb_menu__heading__text"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_timestamp__heading"
            android:layout_width="@dimen/fse__transaction_timestamp__width"
            android:layout_height="match_parent"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_timestamp__heading"
            android:textColor="@color/gcg__thumb_menu__heading__text"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_community_member__heading"
            android:layout_width="@dimen/fse__transaction_community_member__width"
            android:layout_height="match_parent"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_community_member__heading"
            android:textColor="@color/gcg__thumb_menu__heading__text"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_type__heading"
            android:layout_width="@dimen/fse__transaction_type__width"
            android:layout_height="match_parent"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_type__heading"
            android:textColor="@color/gcg__thumb_menu__heading__text"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__document_section__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__document_section__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_new__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_new__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_modified__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_modified__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_deleted__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_deleted__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_unchanged__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_unchanged__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_locked__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_locked__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />

        <TextView
            android:id="@+id/fse__transaction_paragraphs_unlocked__heading"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/menu_heading__background"
            android:gravity="center_horizontal"
            android:paddingTop="7dp"
            android:text="@string/fse__transaction_paragraphs_unlocked__heading"
            android:textColor="@color/yellow"
            android:textSize="12.0dp" />
        
    </LinearLayout>

    <FrameLayout
        android:id="@+id/fse__transaction_scrollview__frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/portobello" >

        <LinearLayout
            android:id="@+id/fse__transaction_scrollview__background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/fse__transaction_number__spacer"
                android:layout_width="@dimen/fse__transaction_number__width"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:background="@color/sand_dollar"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/fse__transaction_timestamp__spacer"
                android:layout_width="@dimen/fse__transaction_timestamp__width"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:background="@color/gcg__tree_view__background"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/fse__transaction_community_member__spacer"
                android:layout_width="@dimen/fse__transaction_community_member__width"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:background="@color/sand_dollar"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/fse__transaction_transaction_type__spacer"
                android:layout_width="@dimen/fse__transaction_type__width"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:background="@color/gcg__tree_view__background"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/fse__transaction_transaction_detail__spacer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="0dp"
                android:background="@color/sand_dollar"
                android:orientation="vertical" />
        </LinearLayout>

        <ScrollView
            android:id="@+id/fse__transaction__scrollview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:fadeScrollbars="false"
            tools:ignore="UselessParent" >

            <com.flywheelms.library.fse.views.FseTransactionTableView
                android:id="@+id/fse__transaction_table__view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" />
        </ScrollView>
    </FrameLayout>

</LinearLayout>
