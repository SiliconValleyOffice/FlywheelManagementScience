<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/audit_block_view__width"
    android:layout_height="@dimen/audit_block_view__height"
    android:background="@color/gcg__tree_view__background"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/audit_block__heading"
        style="@style/GroupBoxLabel"
        android:text="@string/fmm_node_definition__node_frag__audit_block__label_text" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/audit_block__created_by__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__created_by" />

            <TextView
                android:id="@+id/audit_block__created_timestamp__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__created_timestamp" />

            <TextView
                android:id="@+id/audit_block__last_updated_by__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__last_updated_by" />

            <TextView
                android:id="@+id/audit_block__last_updated_timestamp__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__last_updated_timestamp" />

            <TextView
                android:id="@+id/audit_block__locked_by__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__locked_by" />

            <TextView
                android:id="@+id/audit_block__locked_timestamp__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__locked_timestamp" />

            <TextView
                android:id="@+id/audit_block__locked_status__label"
                style="@style/NodeLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingLeft="10dp"
                android:text="@string/audit_block__locked_status" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/audit_block__created_by__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__created_by" />

            <TextView
                android:id="@+id/audit_block__created_timestamp__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__created_timestamp" />

            <TextView
                android:id="@+id/audit_block__last_updated_by__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__last_updated_by" />

            <TextView
                android:id="@+id/audit_block__last_updated_timestamp__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__last_updated_timestamp" />

            <TextView
                android:id="@+id/audit_block__locked_by__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__locked_by" />

            <TextView
                android:id="@+id/audit_block__locked_timestamp__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__locked_timestamp" />
            
            <TextView
                android:id="@+id/audit_block__locked_status__data"
                style="@style/NodeData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
		        android:paddingRight="10dp"
                android:text="@string/audit_block__locked_status" />
        </LinearLayout>
        
    </LinearLayout>

</LinearLayout><!-- From: file:/home/sstamps/AndroidDevelopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/fms__node_frag_audit_block__view.xml -->