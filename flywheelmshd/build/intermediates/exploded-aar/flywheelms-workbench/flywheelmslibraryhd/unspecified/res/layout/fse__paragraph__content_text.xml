<?xml version="1.0" encoding="utf-8"?>
<com.flywheelms.library.fse.views.FseParagraphViewWithTextContent
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="horizontal" >
    
    <ImageView 
        android:layout_width="@dimen/fse__paragraph_menu__vertical_markup__width"
        android:layout_height="match_parent"
        android:background="@color/pdf__transparent"
        android:contentDescription="Sequence Modification State" />
    
    <ImageView 
        android:layout_width="@dimen/fse__paragraph_menu__vertical_markup_bordered__width"
        android:layout_height="match_parent"
        android:background="@color/pdf__transparent"
        android:contentDescription="Lock Modification State" />
    
    <ImageView 
        android:layout_width="@dimen/fse__paragraph_menu__content_modification_state__width"
        android:layout_height="wrap_content"
        android:src="@drawable/gcg__null_drawable"
        android:contentDescription="Content Modification State" />
    
    <ImageView 
        android:layout_width="@dimen/fse__paragraph_menu__vertical_markup__width"
        android:layout_height="match_parent"
        android:background="@color/pdf__transparent"
        android:contentDescription="Numbering Modification State" />
    
    <ImageView 
        android:layout_width="@dimen/fse__paragraph_menu__vertical_markup_bordered__width"
        android:layout_height="match_parent"
        android:background="@color/pdf__transparent"
        android:contentDescription="Style Modification State" />
    
	<com.flywheelms.library.fse.widget.FseParagraphSpinner
	    xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="@dimen/fse__paragraph_menu__spinner__width2"
		android:layout_height="match_parent"
		android:background="@color/sand_dollar"
		android:gravity="right"
		android:paddingRight="7dp" />
    
	<com.flywheelms.library.fse.views.FseParagraphContentTextView
	    xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/fse__document_background"
		android:textColor="@color/fse__document_text"
		android:textCursorDrawable="@drawable/gcg__cursor" />

</com.flywheelms.library.fse.views.FseParagraphViewWithTextContent><!-- From: file:/home/sstamps/AndroidDevelopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout/fse__paragraph__content_text.xml -->