<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gcg="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/gcg__left_menu__width"
    android:layout_height="match_parent"
    android:background="@color/fdk__keypad__background" >

    <include
        android:id="@+id/fdk__left_keypad__container"
        layout="@layout/fdk__left_keypad__container" />

    <LinearLayout
        android:id="@+id/fdk__left_keypad__peer_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/menu_background"
        android:orientation="vertical" >

        <Spinner
            android:id="@+id/frame_spinner"
            android:layout_width="match_parent"
            android:layout_height="@dimen/menu__label_height"
            android:background="@drawable/spinnable_menu_background__left__1_of_3"
            android:gravity="left"
            android:spinnerMode="dropdown"
            android:textColor="@color/White"
            android:textSize="14.0dp" />

        <LinearLayout
            android:layout_width="@dimen/gcg__left_menu__width"
            android:layout_height="match_parent"
            android:orientation="horizontal" >

            <FrameLayout
                android:layout_width="@dimen/gcg__left_menu__subpanel__width"
                android:layout_height="match_parent" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="12dp"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:id="@+id/left_menu__spinner_frame"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/gcg__floating_thumb_menu__height"
                        android:layout_marginTop="9dp" >

                        <RelativeLayout
                            android:id="@+id/perspective_menu__fse__frame_0"
                            android:layout_width="@dimen/gcg__left_menu__subpanel__width"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" >

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/fse_button__story"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:drawableStart="@drawable/button_state_list__fse__story"
                                gcg:frameNumber="0"
                                gcg:perspectiveNumber="0" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/fse_button__notes"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/fse_button__story"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__fse__notes"
                                gcg:frameNumber="0"
                                gcg:perspectiveNumber="1" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/fse_button__history"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/fse_button__notes"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__fse__history"
                                gcg:frameNumber="0"
                                gcg:perspectiveNumber="2" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/fse_button__community"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/fse_button__history"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__fse__contributors"
                                gcg:frameNumber="0"
                                gcg:perspectiveNumber="3" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/perspective_menu__tribkn__frame_1"
                            android:layout_width="@dimen/gcg__left_menu__subpanel__width"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:orientation="vertical" >

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/tribkn_button__deckangl"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="22dp"
                                android:drawableStart="@drawable/button_state_list__perspective__deckangl"
                                gcg:frameNumber="1"
                                gcg:perspectiveNumber="0" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/tribkn_button__governance"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tribkn_button__deckangl"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__tribkn__governance"
                                gcg:frameNumber="1"
                                gcg:perspectiveNumber="1" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/tribkn_button__commitments"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tribkn_button__governance"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__tribkn__commitments"
                                gcg:frameNumber="1"
                                gcg:perspectiveNumber="2" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/tribkn_button__community"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/tribkn_button__commitments"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginBottom="@dimen/menu_button__bottom_margin"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__perspective__community"
                                gcg:frameNumber="1"
                                gcg:perspectiveNumber="3" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/perspective_menu__context__frame_2"
                            android:layout_width="@dimen/gcg__left_menu__subpanel__width"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:orientation="vertical" >

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/context_button__strategic_planning"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="22dp"
                                android:drawableStart="@drawable/button_state_list__context__strategic_planning"
                                gcg:frameNumber="2"
                                gcg:perspectiveNumber="0" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/context_button__work_breakdown"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/context_button__strategic_planning"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__context__work_breakdown"
                                gcg:frameNumber="2"
                                gcg:perspectiveNumber="1" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/context_button__work_planning"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/context_button__work_breakdown"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__context__work_planning"
                                gcg:frameNumber="2"
                                gcg:perspectiveNumber="2" />

                            <com.flywheelms.library.gcg.menu.GcgPerspectiveMenuButton
                                android:id="@+id/context_button__analysis"
                                style="?android:attr/borderlessButtonStyle"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:layout_below="@id/context_button__work_planning"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="9dp"
                                android:drawableStart="@drawable/button_state_list__context__analysis"
                                gcg:frameNumber="2"
                                gcg:perspectiveNumber="3" />
                        </RelativeLayout>
                    </FrameLayout>

                    <com.flywheelms.library.fse.widget.FseMultiShiftButton
                        android:id="@+id/multi_shift__left_button"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/menu_curtain"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/gcg__thumb_menu__background" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/left_menu__filler"
                android:layout_width="6dp"
                android:layout_height="match_parent" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout><!-- From: file:/home/sstamps/AndroidDevevlopment/flywheelms-workbench/flywheelmslibraryhd/src/main/res/layout-land/fmm_node__editor__left_thumbpad.xml -->