<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="0dp"
    android:background="@color/activity_background"
    android:orientation="vertical" >
                
        <TableLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/deckangl__glyph"
            android:layout_width="@dimen/group_box__width__deckangl_glyph"
            android:layout_height="@dimen/group_box__height__noun_status"
            android:layout_gravity="center"
            android:background="@drawable/view_border__title_bar__white"
            android:orientation="vertical"
            android:paddingBottom="@dimen/group_box__padding_bottom"
            android:paddingLeft="@dimen/group_box__padding_left"
            android:paddingRight="@dimen/group_box__padding_right"
            android:paddingTop="@dimen/group_box__padding_top" >

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:padding="0dp" >

                <TextView
                    style="@style/GroupLabel"
                    android:text="@string/deckangl_tm__glyph" />
            </TableRow>

            
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:padding="5dip" >
                
                <LinearLayout 
                    android:id="@+id/deckangl_glyph_scale"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:layout_gravity="center|left"
                android:orientation="vertical"
                android:padding="5dip" >

                    <TextView
                        android:id="@+id/deckangl_glyph__scale__label"
                        android:layout_gravity="left"
                        style="@style/NodeLabelSmall" />

                    <Spinner
                        android:id="@+id/deckangl_glyph__scale__spinner"
                        android:layout_gravity="left"
                        style="@style/NodeDataWithDrawable" />
                </LinearLayout>
                
                <LinearLayout 
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_toRightOf="@id/deckangl_glyph_scale"
                android:layout_centerInParent="true"
                android:padding="5dip" >
                
                <ImageView
                    android:id="@+id/deckangl_glyph__image_view"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                    android:contentDescription="@string/deckangl_tm__glyph" />
                </LinearLayout>
                
                </RelativeLayout>
                
        </TableLayout>

      <TableLayout
          xmlns:android="http://schemas.android.com/apk/res/android"
          android:id="@+id/deckangl__noun_status"
          android:layout_width="340dp"
          android:layout_height="@dimen/group_box__height__noun_status"
          android:layout_marginTop="0dp"
          android:layout_gravity="center"
          android:background="@drawable/view_border__title_bar__white"
          android:orientation="vertical"
          android:paddingBottom="@dimen/group_box__padding_bottom"
          android:paddingLeft="@dimen/group_box__padding_left"
          android:paddingRight="@dimen/group_box__padding_right"
          android:paddingTop="@dimen/group_box__padding_top" >

          <TableRow
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_marginTop="0dp"
              android:gravity="center_horizontal"
              android:padding="0dp" >

              <TextView
                  style="@style/GroupLabel"
                  android:text="@string/deckangl_tm__noun" />
          </TableRow>

          <TableRow
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:paddingBottom="5dp"
              android:paddingLeft="25dp"
              android:paddingRight="5dp"
              android:paddingTop="25dip" >

              <TextView
                  android:id="@+id/deckangl_noun__label"
                  style="@style/NodeLabelSmall" />

              <Spinner
                  android:id="@+id/deckangl_noun__spinner"
                  style="@style/NodeDataWithDrawable" />
          </TableRow>

          <TableRow
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:paddingBottom="5dp"
              android:paddingLeft="25dp"
              android:paddingRight="5dp"
              android:paddingTop="5dip" >

              <TextView
                  android:id="@+id/deckangl__noun_state__label"
                  style="@style/NodeLabelSmall" />

              <Spinner
                  android:id="@+id/deckangl__noun_state__spinner"
                  style="@style/NodeDataWithDrawable" />
          </TableRow>

          <TableRow
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:paddingBottom="5dp"
              android:paddingLeft="25dp"
              android:paddingRight="5dp"
              android:paddingTop="5dip" >

              <TextView
                  android:id="@+id/deckangl_noun__node_quality__label"
                  android:drawableLeft="@drawable/deckangl__noun_quality__good"
                  android:drawablePadding="8dp"
                  android:text="@string/deckangl__noun_quality_index"
                  style="@style/NodeLabelSmall" />

              <TextView
                  android:id="@+id/deckangl_noun__noun_quality__data"
                  android:text="Test"
                  android:paddingLeft="6dp"
                  style="@style/NodeData" />
          </TableRow>
      </TableLayout>

